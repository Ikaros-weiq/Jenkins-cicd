pipeline {
	agent any
	tools {
    	maven 'my_mvn'
	}
	stages {
	    	stage('clean') {
        	steps {
        	sh "mvn clean"  	 
        	}
    	}
    		stage('Build') {
        	steps {
        	sh "mvn compile"  	 
        	}
    	}
   	 
    		stage("Unit test") {          	 
        	steps {  	 
            	sh "mvn test"          	 
       	}
}
}
}
